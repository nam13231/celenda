{
  "id": "/lich-lam-viec",
  "name": "Lịch Làm Việc",
  "short_name": "Lịch",
  "description": "Ứng dụng lịch làm việc đơn giản, lưu nhiệm vụ hàng ngày trên thiết bị (offline).",
  "start_url": "./index.html",
  "scope": "./",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone"],
  "background_color": "#FFFFFF",
  "theme_color": "#4CAF50",
  "orientation": "portrait-primary",
  "lang": "vi",
  "dir": "ltr",
  "icons": [
    {
      "src": "icons/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "icons/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "icons/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "icons/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "screenshots/screen1.png",
      "sizes": "1080x1920",
      "type": "image/png"
    },
    {
      "src": "screenshots/screen2.png",
      "sizes": "1080x1920",
      "type": "image/png"
    }
  ],
  "categories": ["productivity", "utilities"],
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "shortcuts": [
    {
      "name": "Lịch hôm nay",
      "short_name": "Hôm nay",
      "url": "./index.html?today=true",
      "icons": [
        {
          "src": "icons/icon-192.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+lich",
      "url": "./index.html?lich=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "./open-file",
      "accept": {
        "text/plain": [".txt"],
        "application/json": [".json"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "scope_extensions": [
    {
      "origin": "https://celenda.pages.dev"
    }
  ],
  "iarc_rating_id": "placeholder-iarc-id-12345678",
  "prefer_related_applications": false,
  "related_applications": []
}
